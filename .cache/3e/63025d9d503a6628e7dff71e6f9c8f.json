  </div>\n</template>\n\n<script>\nimport { mapActions, mapState, mapMutations } from 'vuex'\n\nexport default {\n  name: 'Login',\n  created () {\n    // load the auth options\n    if (this.user !== null) {\n        if (this.logged) {\n          this.$router.push({ name: 'home' })\n        }\n    }\n  },\n  data: () => {\n    return {\n      form: {\n        email: '',\n        password: ''\n      },\n      ...mapState({\n        user: state => state.c3s.user.currentUser,\n      })\n    }\n  },\n  methods: {\n    ...mapActions('user', [\n      'getLoginOptions',\n      'getAccountProfile',\n      'signIn'\n    ]),\n    ...mapMutations('notification', [\n      'showInfo', 'showError'\n    ]),\n\n    onSubmit () {\n      this.signIn(this.form).then(() => {\n        if (this.user) {\n          this.showInfo({\n            title: 'Welcome',\n            content: 'We are happy to see you again!'\n          })\n          this.$router.push({ name: 'home' })\n        } else {\n          this.showError({ title: 'Wrong credentials', content: 'Your email and/or your password are incorrect' })\n        }\n      })\n    }\n  },\n  computed: {\n    ...mapState('user', [\n      'logged'\n    ])\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"},"lineCount":null}},"error":null,"hash":"47f85a66ee700e5a494f5f4919b44e7b","cacheData":{"env":{}}}