SoundClassificationTemplate.js","original":{"line":117,"column":15}},{"generated":{"line":65,"column":0},"source":"components/Task/Template/Sound/SoundClassificationTemplate.js","original":{"line":115,"column":11}},{"generated":{"line":66,"column":0},"source":"components/Task/Template/Sound/SoundClassificationTemplate.js","original":{"line":3,"column":2}},{"generated":{"line":66,"column":1},"source":"components/Task/Template/Sound/SoundClassificationTemplate.js","original":{"line":2,"column":0}},{"name":"component","generated":{"line":67,"column":15},"source":"components/Task/Template/Sound/SoundClassificationTemplate.js","original":{"line":123,"column":15}}],"sources":{"components/Task/Template/Sound/SoundClassificationTemplate.js":"// eslint-disable-next-line no-unused-vars\nconst component =\n  {\n    template: `\n      <!-- This template use https://bootstrap-vue.js.org/ -->\n\n      <b-row v-if=\"pybossa.userProgressInPercent < 100\">\n        \n        <!-- Form -->\n        <b-col md=\"4\" class=\"mt-4 mt-md-0 order-2 order-md-1\">\n          \n          <b-form-group :key=\"key\" v-for=\"(question, key) in questions\" :label=\"question.question\" label-size=\"lg\">\n          \n            <b-form-radio-group \n              v-model=\"answers[key]\"\n              buttons\n              button-variant=\"outline-primary\"\n              :options=\"question.answers\">\n            </b-form-radio-group>\n            \n          </b-form-group>\n          \n          <b-btn @click=\"submit\" variant=\"success\">Submit</b-btn>\n          \n          <!-- Form validation errors -->\n          <b-alert variant=\"danger\" v-model=\"showAlert\" class=\"mt-2\" dismissible>\n            You must complete the form to submit\n          </b-alert>\n          \n          <!-- User progress -->\n          <p class=\"mt-2\">You are working now on task: <b-badge variant=\"warning\">{{ task.id }}</b-badge></p>\n          <p>You have completed: <b-badge variant=\"primary\">{{ pybossa.userProgress.done }}</b-badge> tasks from <b-badge variant=\"primary\">{{ pybossa.userProgress.total }}</b-badge></p>\n            \n          <b-progress :value=\"pybossa.userProgressInPercent\" :max=\"100\"></b-progress>\n        </b-col>\n        \n        <!-- Sound -->\n        <b-col md=\"8\" class=\"order-1 order-md-2\">\n          <div v-if=\"pybossa.taskLoaded\">\n            <audio v-if=\"taskInfo.audio_url\" :src=\"taskInfo.audio_url\" controls></audio>\n            <div v-else-if=\"taskInfo.embed\" v-html=\"taskInfo.embed\"></div>\n            <b-alert v-else :show=\"true\" variant=\"danger\">Audio media not available</b-alert>\n          </div>\n          <div v-else class=\"text-center\">\n            <b-spinner style=\"width: 4rem; height: 4rem;\" variant=\"primary\" label=\"Audio media loading...\"></b-spinner>\n          </div>\n        </b-col>\n      </b-row>\n      \n      <!-- Task end message -->\n      <b-row v-else>\n        <b-col>\n          <b-jumbotron header=\"This the end!\" lead=\"Thanks you for your participation\"></b-jumbotron>\n        </b-col>\n      </b-row>`,\n\n    data: {\n      questions: [\n        {\n          question: 'What kind of music do you hear?',\n          answers: [\n            'Rock',\n            'Jazz',\n            'Electro',\n            'Hip-Hop'\n          ]\n        },\n        {\n          question: 'What is your tempo estimation?',\n          answers: [\n            '< 50bpm',\n            'Between 50 and 100bpm',\n            'Between 100 and 200bpm',\n            '> 200bpm'\n          ]\n        }\n      ],\n      answers: [],\n      showAlert: false\n    },\n\n    methods: {\n      submit () {\n        if (this.isFormValid()) {\n          this.pybossa.saveTask(this.answers)\n          this.showAlert = false\n          this.answers = []\n          this.questions.forEach(() => this.answers.push(null))\n        } else {\n          this.showAlert = true\n        }\n      },\n      isFormValid () {\n        return this.answers.length === this.questions.length && !this.answers.some(el => typeof el === 'undefined' || el == null)\n      }\n    },\n\n    computed: {\n      task () {\n        return this.pybossa.task\n      },\n      taskInfo () {\n        return this.task.info\n      }\n    },\n\n    created () {\n      this.questions.forEach(() => this.answers.push(null))\n    },\n\n    mounted () {\n      this.pybossa.run()\n    },\n\n    props: {\n      /* Injected by the Pybossa App */\n      pybossa: {\n        required: true\n      }\n    }\n  }\n\nexport default component\n"},"lineCount":null}},"error":null,"hash":"8ce0679e635d951c10b1ccd0df476eac","cacheData":{"env":{}}}