{
  "name": "project-builder",
  "version": 2,
  "routes": [
    {
      "src": "/(js|css|img)/.*",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/" }
  ],
  "alias": "project-builder.staging.citizenscience.ch"
}

